{
    "name": "itarget/finance",
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src\\"
        }
    },
    "autoload-dev": {
